<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ActionBar.Transparent.TMessages.Item" parent="@android:style/TextAppearance.Widget.IconMenu.Item">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="ActionBar.Transparent.TMessages.Start" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">#ffffffff</item>
        <item name="android:title"></item>
        <item name="android:logo">@drawable/transparent</item>
    </style>
    <style name="DialogNoAnimation">
        <item name="android:windowEnterAnimation">@anim/no_animation</item>
        <item name="android:windowExitAnimation">@anim/no_animation</item>
    </style>
    <style name="Theme.TMessages" parent="@android:style/Theme.Holo.Light">
        <item name="android:colorBackground">@android:color/white</item>
        <item name="android:windowBackground">@android:color/white</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:editTextStyle">@style/Theme.TMessages.EditText</item>
        <item name="android:listViewStyle">@style/Theme.TMessages.ListView</item>
        <item name="android:itemTextAppearance">@style/ActionBar.Transparent.TMessages.Item</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowActionBarOverlay">false</item>
        <item name="android:listChoiceBackgroundIndicator">@drawable/list_selector</item>
        <item name="android:actionBarItemBackground">@drawable/bar_selector_style</item>
    </style>
    <style name="Theme.TMessages.EditText" parent="@android:style/Widget.EditText">
        <item name="android:textColor">#ff000000</item>
        <item name="android:background">@drawable/edit_text</item>
    </style>
    <style name="Theme.TMessages.ListView" parent="@android:style/Widget.ListView">
        <item name="android:fadingEdge">none</item>
        <item name="android:listSelector">@drawable/list_selector</item>
        <item name="android:divider">@color/divider</item>
        <item name="android:dividerHeight">1.0dip</item>
    </style>
    <style name="Theme.TMessages.PopupNotification" parent="@style/Theme.TMessages">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowActionBar">false</item>
    </style>
    <style name="Theme.TMessages.Start" parent="@android:style/Theme.Holo.Light">
        <item name="android:colorBackground">@android:color/white</item>
        <item name="android:windowBackground">@android:color/white</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:actionBarStyle">@style/ActionBar.Transparent.TMessages.Start</item>
    </style>
    <style name="Theme.TMessages.Transparent" parent="@style/Theme.TMessages">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowCloseOnTouchOutside">false</item>
    </style>
    <style name="TransparentDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:background">@null</item>
        <item name="android:windowFullscreen">false</item>
    </style>
    <style name="AboutItemTextStyle">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="ActionBar">
        <item name="android:textSize">@dimen/ui_font_size</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="ActionBar.Bar" parent="@style/ActionBar">
        <item name="android:background">#ff000000</item>
    </style>
    <style name="ActionBar.Button" parent="@android:style/Widget.Button.Small">
        <item name="android:textSize">@dimen/ui_font_size</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:paddingLeft">8.0dip</item>
        <item name="android:paddingRight">8.0dip</item>
        <item name="android:layout_margin">2.0dip</item>
    </style>
    <style name="ActionBar.Icon" parent="@style/ActionBar">
        <item name="android:background">@drawable/action_button_bg</item>
        <item name="android:paddingTop">@dimen/action_button_padding_v</item>
        <item name="android:paddingBottom">@dimen/action_button_padding_v</item>
        <item name="android:layout_width">@dimen/action_button_width</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:scaleType">centerInside</item>
    </style>
    <style name="ActionBar.Menu" parent="@style/ActionBar">
        <item name="android:gravity">center|right|top</item>
        <item name="android:background">#ff000000</item>
    </style>
    <style name="ActionBar.MenuItem" parent="@style/ActionBar">
        <item name="android:textSize">@dimen/ui_font_size</item>
        <item name="android:gravity">left</item>
        <item name="android:background">@drawable/action_button_bg</item>
        <item name="android:paddingLeft">16.0dip</item>
        <item name="android:paddingTop">8.0dip</item>
        <item name="android:paddingRight">16.0dip</item>
        <item name="android:paddingBottom">8.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">0.0dip</item>
    </style>
    <style name="ActionBar.Text" parent="@style/ActionBar">
        <item name="android:textSize">@dimen/title_size</item>
        <item name="android:background">@null</item>
    </style>
    <style name="ActionModeStyle" parent="@android:style/Theme.Holo.Light">
        <item name="android:background">#ffffffff</item>
    </style>
    <style name="LockActivityTheme" parent="@style/Theme.TMessages.Start">
        <item name="android:buttonStyle">@style/Theme_Lock_Button</item>
        <item name="android:imageButtonStyle">@style/Theme_Lock_ImageButton</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="PopupAnimation" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/popup_in</item>
        <item name="android:windowExitAnimation">@anim/popup_out</item>
    </style>
    <style name="Theme_Lock_Button" parent="@android:style/Widget.Button">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:background">@drawable/passwordview_button_background</item>
        <item name="android:padding">16.0dip</item>
    </style>
    <style name="Theme_Lock_ImageButton" parent="@android:style/Widget.ImageButton">
        <item name="android:textColor">#ffff0000</item>
        <item name="android:background">@drawable/passwordview_button_background</item>
        <item name="android:padding">16.0dip</item>
    </style>
    <style name="TransparentDialogWithActionMode" parent="@style/TransparentDialog">
        <item name="android:actionModeStyle">@style/ActionModeStyle</item>
    </style>
    <style name="mdtp_ActionButton">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:focusable">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/mdtp_material_button_height</item>
    </style>
    <style name="mdtp_ActionButton.Text" parent="@style/mdtp_ActionButton">
        <item name="android:textSize">@dimen/mdtp_material_button_textsize</item>
        <item name="android:textColor">@color/mdtp_button_color</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:paddingLeft">@dimen/mdtp_material_button_textpadding_horizontal</item>
        <item name="android:paddingRight">@dimen/mdtp_material_button_textpadding_horizontal</item>
        <item name="android:minWidth">@dimen/mdtp_material_button_minwidth</item>
        <item name="android:singleLine">true</item>
        <item name="android:textAllCaps">true</item>
    </style>
    <style name="mdtp_ampm_label">
        <item name="android:textSize">@dimen/mdtp_ampm_label_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/mdtp_ampm_text_color</item>
        <item name="android:textAllCaps">true</item>
    </style>
    <style name="mdtp_day_of_week_label_condensed" />
    <style name="mdtp_time_label">
        <item name="android:textSize">@dimen/mdtp_time_label_size</item>
        <item name="android:textColor">@color/mdtp_numbers_text_color</item>
    </style>
    <style name="myBackgroundStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
</resources>
